{
  "compilerOptions": {
    "target": "ES2016",
    "lib": ["DOM", "DOM.Iterable", "ES2016"],
    "jsx": "react",
    "module": "ESNext",
    "moduleResolution": "Node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noEmit": true
  },
  "include": ["src"]
}